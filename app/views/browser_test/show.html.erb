<% if flash[:notice] %>
  <div class="row" id="flash" style="padding-left: 30px;">
    <div class"span12">
      <div class="alert alert-success text-center" id="notice">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        <h4><%= flash[:notice] %></h4>
      </div>
    </div>
  </div>
<% elsif flash[:error] %>
  <div class="row" id="flash">
    <div class="span12">
      <div class="alert alert-error text-center" id="error">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        <h4><%= flash[:error] %></h4>
      </div>
    </div>
  </div>
<% end %>

